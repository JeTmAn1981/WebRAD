    Private Sub btnContinue_Click(ByVal sender As System.Object, ByVal e As System.EventArgs) Handles btnContinue.Click
            Page.Validate()

            If Page.IsValid Then
			%(RunAOBeforePageSubmitCall)%
			
            Dim sLastName, sfirstname, smiddlename, sprefix, semail, sphonenumber, sphoneext, sphonetype, saddress1, saddress2, scity, sstate, szip, scountry, sWhitworthID As String
            Dim nCurrentID As Integer
            Dim cmd As New SqlCommand
            
            If CDec(lblUSBamountdue.Text) > 0 Then
                dim invoiceVariables as Dictionary(Of string, InvoiceVariable) = LoadInvoiceVariables()
                lblUSBtransactionkey.Text = InsertInvoice(invoicevariables, lblInvoicetype.Text)
            End If
		
            cmd.connection = cnx
            cmd.CommandType = CommandType.StoredProcedure
  	        cmd.commandtext = "usp_%(MainStoredProcedureType)%%(MainStoredProcedureName)%"

                With cmd
		    %(MainStoredProcedureParameters)%
                    cmd.Parameters.AddWithValue("@Invoice", lblUSBtransactionkey.Text)
                End With

   	        nCurrentID = ExecuteScalar(cmd, cnx)	
			Session("ApplicationID") = nCurrentID
			nSubmissionID = nCurrentID
			
    	    %(DeleteAncillaryData)%
	        %(SaveAncillaryCalls)%
			%(DeleteTempFiles)%
     	    %(SendEmailCall)%%(RunAOAfterPageSubmitCall)%

			If CDec(lblUsbAmountDue.text) > 0 And InvoiceContainsGLCharges(lblUSBtransactionkey.Text) Then
				RedirectUserToEcommercePaymentPage(lblUSBtransactionkey.Text)
			Else
				%(Redirect)%
			End If
                
            End If
        End Sub 'btnContinue_Click

