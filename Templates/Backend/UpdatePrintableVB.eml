		dim dtSupplied as new WhitTools.DataTablesSupplied
	
       %(LoadDDLsContent)%
    End Sub
  
    Sub BindData()
	dim dt as new DataTable

        dt = getdatatable("Select * From %(MainTableName)% Where ID = '" & %(IDSelect)% & "'",cnx)

	If dt.rows.count > 0
	    With dt.rows(0)
        	%(BindData)%

        	%(TotalOwed)%

			%(TriggerPostbackActions)%

			%(BindRepeaterData)%
			
			%(BindDataAdditional)%
			
			%(BindWorkflowSteps)%
            End With
        End If
    End Sub

